    "@reduxjs/toolkit": "^1.3.6",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.0.2",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.3",
    "@types/yup": "^0.29.13",
    "axios": "^0.21.1",
    "connected-react-router": "^6.8.0",
    "formik": "^2.1.4",
    "history": "^4.10.1",
    "lint-staged": "^10.2.2",
    "notistack": "^0.9.17",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "4.0.3",
    "react-slick": "^0.26.1",
    "redux": "^4.0.5",
    "redux-mock-store": "^1.5.4",
    "redux-persist": "^6.0.0",
